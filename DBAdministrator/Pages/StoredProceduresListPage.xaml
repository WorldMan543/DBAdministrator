<Page x:Class="DBAdministrator.Pages.StoredProceduresListPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	  DataContext="{Binding RelativeSource={RelativeSource Self}}"
	Title="StoredProceduresListPage">

    <Grid>
		<ListView ItemsSource="{Binding Models}" Margin="15">
			<ListView.View>
				<GridView>
					<GridViewColumn Header="Name" Width="120" DisplayMemberBinding="{Binding ProcedureName}" />
					<GridViewColumn Header="Owner" Width="120" DisplayMemberBinding="{Binding Owner}" />
					<GridViewColumn Header="Create Date" Width="120" DisplayMemberBinding="{Binding CreateDate}" />
					<GridViewColumn Header="Type" Width="120" DisplayMemberBinding="{Binding Type}" />
				</GridView>
			</ListView.View>
		</ListView>
	</Grid>
</Page>
